<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="900.0" prefWidth="1350.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PractiseSelection">
   <children>
      <ImageView fitHeight="906.0" fitWidth="1356.0" layoutX="-1.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../background.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="89.0" fitWidth="1264.0" layoutX="50.0" layoutY="115.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../practisePrompt.jpeg" />
         </image>
      </ImageView>
      <ImageView fx:id="arrowBtn" fitHeight="111.0" fitWidth="146.0" layoutX="11.0" layoutY="12.0" onMouseClicked="#backToMenu" onMouseEntered="#enterArrow" onMouseExited="#exitArrow" pickOnBounds="true" preserveRatio="true" rotate="180.0">
         <image>
            <Image url="@../arrow.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="randomBtn" fitHeight="442.0" fitWidth="526.0" layoutX="179.0" layoutY="371.0" onMouseEntered="#enterRandom" onMouseExited="#exitRandom" onMousePressed="#chooseRandom" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../randomisefade.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="selectBtn" fitHeight="442.0" fitWidth="484.0" layoutX="739.0" layoutY="371.0" onMouseClicked="#chooseSelect" onMouseEntered="#enterSelect" onMouseExited="#exitSelect" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../selectfade.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
